<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js" type="text/javascript"></script>
<script>
    var data = (function ($) { 
        var getData = (function() {
            $.ajax({
                method: "GET",
                data: "id=" + 1,
                url: "@Url.Action("GetPlayer", "Home")",
                dataType: "json",
                success: function (response) {
                    print(response);
                },
                failure: function (response) {
                    alert(response.d);
                }
            });
        });

        function print(player) {
            console.log(JSON.stringify(player, null, 4));
            document.write(JSON.stringify(player, null, 4));
        }

        return {
            getData: getData
        }
    })(jQuery);
</script>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <script>
            data.getData();
        </script>
    </div>
</body>
</html>
